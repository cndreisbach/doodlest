{
  "name": "scribblest",
  "productName": "Scribblest",
  "version": "0.1.2",
  "homepage": "./",
  "main": "src/main/index.js",
  "description": "Whiteboard app with an infinite canvas",
  "repository": "https://github.com/cndreisbach/scribblest.git",
  "keywords": [],
  "author": "Clinton Dreisbach",
  "license": "MIT",
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "build": "yarn compile && electron-builder",
    "build:dir": "yarn build --dir -c.compression=store -c.mac.identity=null"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "^8.2.2",
    "electron": "^11.0.2",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-webpack": "^2.8.2",
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "effector": "^21.7.5",
    "fabric": "^4.2.0",
    "localforage": "^1.9.0",
    "mousetrap": "^1.6.5",
    "redom": "^3.27.1",
    "source-map-support": "^0.5.19"
  },
  "build": {
    "appId": "us.dreisbach.scribblest",
    "productName": "Scribblest",
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.graphics-design"
    },
    "publish": [
      {
        "provider": "github",
        "releaseType": "draft",
        "vPrefixedTagName": true
      }
    ]
  }
}
